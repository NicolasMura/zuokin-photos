{
  "name": "zuokin-photos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "yarn start",
    "start:frontend-public": "nx serve frontend-public --hmr",
    "start:backend-api": "nx serve backend-api",
    "serve:dist": "node node_modules/typescript/bin/tsc apps/frontend-public/src/env-for-prod-service-workers.ts --outfile apps/frontend-public/src/env.js && nx build frontend-public --prod && git checkout apps/frontend-public/src/env.js && http-server -p 8080 -c-1 dist/apps/frontend-public",
    "kill-http-server": "sudo kill -9 $(lsof -ti:8080)",
    "build": "ng build",
    "generate-build-infos": "node node_modules/typescript/bin/tsc scripts/generate-build-infos.ts && node scripts/generate-build-infos.js --no-version --no-message --jenkinsBuildNumber $BUILD_ID",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open --project ./apps/frontend-public-e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "deploy:frontend-public:dockerhub": "nx build frontend-public --prod && docker build -t zuokin-photos-frontend-public -f .docker/Dockerfile.frontend-public . && docker tag zuokin-photos-frontend-public nicolasmura/zuokin-photos-frontend-public && docker push nicolasmura/zuokin-photos-frontend-public",
    "deploy:backend-api:dockerhub": "docker build -t zuokin-photos-backend-api -f .docker/Dockerfile.backend-api . && docker tag zuokin-photos-backend-api nicolasmura/zuokin-photos-backend-api && docker push nicolasmura/zuokin-photos-backend-api",
    "deploy:database:dockerhub": "docker build -t zuokin-photos-database -f .docker/Dockerfile.mongodb . --no-cache && docker tag zuokin-photos-database nicolasmura/zuokin-photos-database && docker push nicolasmura/zuokin-photos-database"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.13",
    "@angular/cdk": "12.2.12",
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/forms": "12.2.13",
    "@angular/material": "12.2.12",
    "@angular/material-moment-adapter": "^12.2.0",
    "@angular/platform-browser": "12.2.13",
    "@angular/platform-browser-dynamic": "12.2.13",
    "@angular/router": "12.2.13",
    "@angular/service-worker": "~12.2.0",
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/platform-ws": "^8.4.4",
    "@nestjs/websockets": "^8.4.4",
    "@nrwl/angular": "13.1.3",
    "bcrypt": "^5.0.1",
    "browser-fs-access": "^0.21.1",
    "exifreader": "^4.5.0",
    "fs": "^0.0.1-security",
    "hammerjs": "^2.0.8",
    "joi": "^17.4.2",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.3",
    "mongoose": "^6.0.12",
    "multer": "^1.4.4",
    "ngx-webstorage": "^8.0.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.13",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@angular/language-service": "12.2.13",
    "@angularclass/hmr": "^3.0.0",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.2",
    "@nrwl/cli": "13.1.3",
    "@nrwl/cypress": "13.1.3",
    "@nrwl/eslint-plugin-nx": "13.1.3",
    "@nrwl/jest": "13.1.3",
    "@nrwl/linter": "13.1.3",
    "@nrwl/nest": "13.1.3",
    "@nrwl/node": "13.1.3",
    "@nrwl/tao": "13.1.3",
    "@nrwl/workspace": "13.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "27.0.2",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "14.14.33",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "cypress": "^7.3.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "prettier": "^2.3.1",
    "signale": "^1.4.0",
    "ts-jest": "27.0.5",
    "ts-node": "~9.1.1",
    "typescript": "4.3.5"
  }
}
